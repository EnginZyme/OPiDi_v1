{
  "name": "protocol-designer-spa",
  "version": "2.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deploy": "aws --profile protocol-designer-spa --region eu-north-1 s3 sync ./dist s3://protocol-designer-spa-test --delete",
    "deploy-prod": "aws --profile protocol-designer-spa --region eu-north-1 s3 sync ./dist s3://protocol-designer-spa --delete",
    "docs:build-markdown": "vue-docgen -c docgen.config.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@simonwep/selection-js": "^2.1.1",
    "ajv": "^8.6.2",
    "aws-sdk": "^2.844.0",
    "axios": "^0.21.1",
    "convert-csv-to-json": "^1.0.2",
    "dotenv": "^10.0.0",
    "file-saver": "^2.0.5",
    "json-stringify-pretty-compact": "^2.0.0",
    "jszip": "^3.5.0",
    "papaparse": "^5.3.0",
    "primeicons": "^4.1.0",
    "primevue": "^2.3.0",
    "slugify": "^1.4.6",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.4.0"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/test-utils": "^1.2.2",
    "@vuese/cli": "^2.14.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest-serializer-vue": "^2.0.2",
    "jsdoc": "^3.6.7",
    "jsdoc-to-markdown": "^7.0.0",
    "jsdoc-vuex-plugin": "^1.0.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "vue-docgen": "0.0.3",
    "vue-docgen-cli": "^4.35.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.8.2",
    "vuepress-bar": "^0.4.2",
    "webpack": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
